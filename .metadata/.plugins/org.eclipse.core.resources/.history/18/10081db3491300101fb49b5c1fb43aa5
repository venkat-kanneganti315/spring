public class creatingObjectInSpring {

	public static void main(String[] args) {
		
		ApplicationContext ctx = new ClassPathXmlApplicationContext("spring.xml");
		Employee emp = (Employee)ctx.getBean("e1");
		System.out.println(emp);
		
		

		
